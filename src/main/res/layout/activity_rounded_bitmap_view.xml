<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.viewdome.ui.activity.imaview.RoundedBitmapViewActivity"
    android:orientation="vertical"
    >

    <include layout="@layout/toolbar_layout"/>

    <com.example.viewdome.ui.view.imagerview.MRoundedBitmapView
        android:id="@+id/mroundedBitmapView"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:src="@mipmap/bg_11"
        app:borderRadius="10dp"
        app:border_inside_color="#ffffff"
        app:border_outside_color="#000000"
        app:border_thickness="1dp"
        app:type="round" />
    <com.example.viewdome.ui.view.imagerview.MRoundedBitmapView
        android:id="@+id/mroundedBitmapView2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:src="@mipmap/bg_11"
        app:borderRadius="20dp"
        app:border_inside_color="#ffffff"
        app:border_outside_color="#000000"
        app:border_thickness="1dp"
        app:type="round" />
    <com.example.viewdome.ui.view.imagerview.MRoundedBitmapView
        android:id="@+id/mroundedBitmapView3"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="center"
        android:layout_marginTop="20dp"
        android:src="@mipmap/bg_11"
        app:borderRadius="10dp"
        app:border_inside_color="#ffffff"
        app:border_outside_color="#000000"
        app:border_thickness="1dp"
        app:type="circle" />


	 <om.example.viewdome.ui.view.progress.CircleProgressView
	                android:id="@+id/circleProgressView"
	                android:layout_centerInParent="true"
	                android:layout_width="@dimen/dimen_main_headportrait_size"
	                android:layout_height="@dimen/dimen_main_headportrait_size"
	                app:roundWidth="@dimen/dimen_main_headValue_circleProgress_width"
	                app:roundColor="@color/coloRroundColor"
	                app:roundProgressColorStart="@color/colorRoundProgressColorStart"
	                app:roundProgressColorEnd="@color/colorRoundProgressColorEnd"
	                />


</LinearLayout>
